+void sys_libc_log(const char *message) {
+	size_t n = 0;
+	while(message[n])
+		n++;
+	do_syscall(SYS_write, 2, message, n);
+	char lf = '\n';
+	do_syscall(SYS_write, 2, &lf, 1);
+}
