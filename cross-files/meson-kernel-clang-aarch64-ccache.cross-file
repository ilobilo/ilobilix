[binaries]
c = ['ccache', 'clang']
cpp = ['ccache', 'clang++']
c_ld = 'lld'
cpp_ld = 'lld'
ar = 'llvm-ar'
strip = 'llvm-strip'
pkg-config = 'pkg-config'
cmake = 'cmake'

[built-in options]
c_args = ['-target', 'aarch64-elf']
cpp_args = ['-target', 'aarch64-elf']

[host_machine]
system = 'none'
cpu_family = 'aarch64'
cpu = 'aarch64'
endian = 'little'
